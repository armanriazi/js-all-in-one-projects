{
	"name": "express-sequelize",
	"version": "0.0.0",
	"description": "This is an example of how to setup Sequelize and Express together in a project for NodeJS 10 and above.",
	"main": "index.js",
	"type": "commonjs",
	"scripts": {
		"postgres:start": "cross-env NODE_ENV=development node ./index.js",
		"postgres:nodemon:start": "cross-env NODE_ENV=development nodemon -r esm ./index.js",
		"db:setup": "node ./database/setup.js"
	},
	"engines": {
		"node": ">=16"
	},
	"license": "MIT",
	"dependencies": {
		"body-parser": "^1.20.2",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"pg": "^8.11.5",
		"sequelize": "^6.37.2",
		"sqlite3": "^5.1.7"
	},
	"devDependencies": {
		"esm": "3.2.25",
		"nodemon": "^3.1.0"
	}
}
